{
  "name": "platziverse-web",
  "version": "1.0.0",
  "description": "A simple IoT Platform - Web",
  "main": "server.js",
  "scripts": {
    "start": "SET NODE_ENV=production && node server.js",
    "prestart": "npm run build",
    "start-dev": "SET DEBUG=platziverse:* && nodemon server.js",
    "prestart-dev": "npm run build",
    "build": "browserify -t babelify -t vueify client/app.js -o public/bundle.js",
    "lint": "standard"
  },
  "author": "",
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "nodemon": "^2.0.9",
    "standard": "^16.0.3",
    "vueify": "^9.4.1"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "chart.js": "^3.4.0",
    "debug": "^3.2.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-asyncify": "^1.0.1",
    "moment": "^2.29.1",
    "platziverse-agent": "file:../platziverse-agent",
    "random-material-color": "^1.0.5",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "vue": "^2.6.14",
    "vue-chartjs": "^3.5.1"
  }
}
